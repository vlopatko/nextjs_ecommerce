{
  "name": "nextjs_ecommerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-all": "npm run lint && npx lint-staged && npm run check-types && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write . && git add -A ."
    }
  },
  "lint-staged": {
    "app/**/*.{ts,tsx,html,css}": "prettier --write",
    "components/**/*.{ts,tsx,html,css}": "prettier --write",
    "app/**/*.{ts,tsx}": "eslint",
    "components/**/*.{ts,tsx}": "eslint"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.11",
    "@types/node": "20.6.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "auth": "^0.0.9",
    "autoprefixer": "^10.4.15",
    "eslint-config-next": "13.4.19",
    "lint-staged": "^14.0.1",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "postcss": "^8.4.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4"
  }
}
